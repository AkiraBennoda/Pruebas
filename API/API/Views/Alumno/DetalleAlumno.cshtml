@model API.Models.Alumnos
@{
    ViewBag.Title = "DetalleAlumno";
}

<h2>Detalle Alumno</h2>
<hr/>


    <div class="form-group">
        <label>ID Alumno: </label>
       @Model.ID
    </div>

    <div class="form-group">
        <label>Nombre: </label>
        @Model.Nombre
    </div>

    <div class="form-group">
        <label>Apellido:</label>
        @Model.Apellido
    </div>

    <div class="form-group">
        <label>Matriicula:</label>
        @Model.Matricula
    </div>
    <div class="form-group">
        
        <a href="@Url.Action("Index", "Alumno")">Regresar</a>
    </div>


